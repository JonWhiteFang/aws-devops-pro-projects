{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyS3PublicACL",
      "Effect": "Deny",
      "Action": [
        "s3:PutBucketAcl",
        "s3:PutObjectAcl"
      ],
      "Resource": "*",
      "Condition": {
        "StringEquals": { "s3:x-amz-acl": ["public-read","public-read-write","authenticated-read"] }
      }
    }
  ]
}
